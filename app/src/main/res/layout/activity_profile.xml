<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/textViewFaveorites"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ProfileActivity"
    tools:layout_editor_absoluteX="41dp"
    tools:layout_editor_absoluteY="250dp">

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/imageViewProfilephoto"
        android:layout_width="151dp"
        android:layout_height="104dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="52dp"
        android:layout_marginEnd="102dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toTopOf="@+id/textView3"
        app:layout_constraintEnd_toStartOf="@+id/textViewFavegenres"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:srcCompat="@drawable/user" />

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/imageViewHomePhotoone"
        android:layout_width="100dp"
        android:layout_height="87dp"
        android:layout_marginStart="66dp"
        android:layout_marginTop="41dp"
        android:layout_marginEnd="46dp"
        android:layout_marginBottom="155dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textViewPhotoSubmissions"
        app:layout_constraintTop_toBottomOf="@+id/textViewSecondfaveaddress"
        tools:srcCompat="@drawable/photo_submitted" />

    <TextView
        android:id="@+id/textViewPhotoSubmissions"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="66dp"
        android:layout_marginBottom="77dp"
        android:text="Your Photo Submissions"
        app:layout_constraintBottom_toTopOf="@+id/buttonEditProfile"
        app:layout_constraintEnd_toStartOf="@+id/imageViewHomePhotoone"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageViewSecondfave" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="27dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="60dp"
        android:layout_marginBottom="27dp"
        android:text="Your Favorite Libraries"
        app:layout_constraintBottom_toTopOf="@+id/imageViewlibuno"
        app:layout_constraintEnd_toStartOf="@+id/textViewFavegenres"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageViewProfilephoto" />

    <TextView
        android:id="@+id/textViewusername"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="134dp"
        android:layout_marginTop="185dp"
        android:layout_marginEnd="48dp"
        android:layout_marginBottom="33dp"
        android:hint="Your Name"
        app:layout_constraintBottom_toTopOf="@+id/textViewFavegenres"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageViewProfilephoto"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="Your Name" />

    <TextView
        android:id="@+id/textViewFavegenres"
        android:layout_width="144dp"
        android:layout_height="37dp"
        android:layout_marginStart="60dp"
        android:layout_marginTop="33dp"
        android:layout_marginEnd="6dp"
        android:layout_marginBottom="53dp"
        android:hint="Your Favorite Genre"
        app:layout_constraintBottom_toTopOf="@+id/textViewFirstfave"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView3"
        app:layout_constraintTop_toBottomOf="@+id/textViewusername" />

    <TextView
        android:id="@+id/textViewFirstfave"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="60dp"
        android:layout_marginTop="36dp"
        android:layout_marginEnd="46dp"
        android:layout_marginBottom="15dp"
        android:hint="Favorite #1"
        app:layout_constraintBottom_toTopOf="@+id/textViewFirstfaveaddress"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageViewlibuno"
        app:layout_constraintTop_toBottomOf="@+id/textViewFavegenres" />

    <TextView
        android:id="@+id/textViewFirstfaveaddress"
        android:layout_width="84dp"
        android:layout_height="33dp"
        android:layout_marginStart="123dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="34dp"
        android:layout_marginBottom="86dp"
        android:hint="Address for Fave #1"
        app:layout_constraintBottom_toTopOf="@+id/textViewSecondfave"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageViewlibuno"
        app:layout_constraintTop_toBottomOf="@+id/textViewFirstfave" />

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/imageViewlibuno"
        android:layout_width="151dp"
        android:layout_height="78dp"
        android:layout_marginStart="26dp"
        android:layout_marginTop="23dp"
        android:layout_marginEnd="76dp"
        android:layout_marginBottom="20dp"
        app:layout_constraintBottom_toTopOf="@+id/imageViewSecondfave"
        app:layout_constraintEnd_toStartOf="@+id/textViewFirstfave"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        tools:srcCompat="@drawable/library_fave" />

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/imageViewSecondfave"
        android:layout_width="122dp"
        android:layout_height="111dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="66dp"
        android:layout_marginBottom="13dp"
        app:layout_constraintBottom_toTopOf="@+id/textViewPhotoSubmissions"
        app:layout_constraintEnd_toStartOf="@+id/textViewSecondfaveaddress"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageViewlibuno"
        tools:srcCompat="@drawable/photo_submission" />

    <TextView
        android:id="@+id/textViewSecondfave"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="137dp"
        android:layout_marginTop="53dp"
        android:layout_marginEnd="46dp"
        android:layout_marginBottom="4dp"
        android:hint="Favorite #2"
        app:layout_constraintBottom_toTopOf="@+id/textViewSecondfaveaddress"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageViewSecondfave"
        app:layout_constraintTop_toBottomOf="@+id/textViewFirstfaveaddress" />

    <TextView
        android:id="@+id/textViewSecondfaveaddress"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="66dp"
        android:layout_marginTop="34dp"
        android:layout_marginEnd="23dp"
        android:layout_marginBottom="41dp"
        android:hint="Address Fave #2"
        app:layout_constraintBottom_toTopOf="@+id/imageViewHomePhotoone"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageViewSecondfave"
        app:layout_constraintTop_toBottomOf="@+id/textViewSecondfave" />

    <Button
        android:id="@+id/buttonEditProfile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="158dp"
        android:layout_marginTop="595dp"
        android:layout_marginEnd="165dp"
        android:layout_marginBottom="30dp"
        android:text="Edit Your Profile"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.444"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

</androidx.constraintlayout.widget.ConstraintLayout>